// Ierators
#include<bits/stdc++.h>
using namespace std;

int main(){
    int n;
    cin>>n;
    vector<int> v(n);
    for(int i=0;i<n;i++){
        cin>>v[i];
    }
    //printing vector
    for(vector<int> :: iterator it = v.begin(); it != v.end(); it++){
        cout<<*it<<" ";
    }
    cout<<endl;
    //reverse the vector
    for(vector<int> :: iterator it = v.end() - 1; it != v.begin() - 1; it--){
        cout<<*it<<" ";
    }
    return 0;
}

